{
  "format": "custom",
  "id": "button_link",
  "label": "Button Link",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "text": {
        "type": "Text",
        "config": {
          "label": "Text",
          "placeholder": ""
        }
      },
      "page_link": {
        "type": "Link",
        "config": {
          "label": "Page Link",
          "placeholder": "",
          "allowTargetBlank": true,
          "select": null,
          "allowText": true
        }
      },
      "new_tab": {
        "type": "Boolean",
        "config": {
          "label": "New Tab",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "variant": {
        "type": "Select",
        "config": {
          "label": "Variant",
          "placeholder": "",
          "options": [
            "primary",
            "secondary",
            "outline",
            "destructive",
            "ghost",
            "link"
          ],
          "default_value": "primary"
        }
      },
      "size": {
        "type": "Select",
        "config": {
          "label": "Size",
          "placeholder": "",
          "options": ["lg", "md", "sm", "icon"]
        }
      }
    }
  }
}
